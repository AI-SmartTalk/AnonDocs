{
  "name": "anondocs-api",
  "version": "1.0.0",
  "description": "Open-source API for anonymizing documents using LLM",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "anonymization",
    "pii",
    "document-processing",
    "langchain",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.2.0",
    "@langchain/openai": "^0.2.0",
    "@langchain/anthropic": "^0.2.0",
    "@langchain/community": "^0.2.0",
    "@prisma/client": "^5.15.0",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.7.2",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.0",
    "@types/cors": "^2.8.17",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prisma": "^5.15.0",
    "tsx": "^4.15.0",
    "typescript": "^5.4.5"
  }
}

